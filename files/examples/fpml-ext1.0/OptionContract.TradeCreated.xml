<?xml version="1.0" encoding="UTF-8"?>
<FpML xmlns="http://www.fpml.org/2005/FpML-4-2" 
	xmlns:ext="http://www.fpml.org/extensions" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://www.fpml.org/2005/FpML-4-2 fpml-main-4-2.xsd http://www.fpml.org/extensions fpml-extensions-1-0.xsd" 
	xsi:type="TradeCreated" 
	version="4-2">
	
	<!-- AP interest rate option based on CBOE specification -->
	<!-- http://www.cboe.com/Products/InterestRateOptionsSpecs.aspx -->
	
	<header>
		<messageId 
			messageIdScheme="http://www.jpmorgan.com/coding-scheme/message-id">345345</messageId>
		<sentBy 
			partyIdScheme="http://www.jpmorgan.com/coding-scheme/party-id">bmf</sentBy>
		<creationTimestamp>2000-08-01T08:57:00-00:00</creationTimestamp>
	</header>
	
	<trade>
		<tradeHeader>
			<partyTradeIdentifier>
				<partyReference href="PartyA"/>
				<tradeId 
					tradeIdScheme="http://www.PartyA.com/coding-scheme/trade-id">1234</tradeId>
			</partyTradeIdentifier>
			<tradeDate id="TradeDate">2005-07-13</tradeDate>
		</tradeHeader>
		
		<ext:optionContract>
		
			<buyerPartyReference href="PartyA"/>
			<sellerPartyReference href="PartyB"/>
			
			<ext:optionType>Put</ext:optionType>
			<ext:exerciseStyle>European</ext:exerciseStyle>
			<!-- strike at 4.50% -->
			<ext:strikePrice>0.0450</ext:strikePrice>
			<!-- stated in decimals, one point equals USD 100 -->
			<ext:optionPremium>500</ext:optionPremium>
			<ext:numberOfContracts>20</ext:numberOfContracts>
			
			<ext:settlementType>Cash</ext:settlementType>
			<ext:settlementDate>
				<relativeDate>
					<periodMultiplier>1</periodMultiplier>
					<period>D</period>
					<dayType>Business</dayType>
					<businessDayConvention>NONE</businessDayConvention>
					<businessCenters>
						<businessCenter>EUTA</businessCenter>
					</businessCenters>
					<dateRelativeTo href="ExpirationDate"/>
				</relativeDate>
			</ext:settlementDate>
			<ext:settlementCurrency>USD</ext:settlementCurrency>
			
			<ext:option>
				<instrumentId
					instrumentIdScheme="http://www.cboe.com/coding-scheme/instrument-id">TNX</instrumentId>
				<description>10 Year Treasury Note Option on CBOE</description>
				<currency>USD</currency>
				<exchangeId>CBOE</exchangeId>
				<!-- multiplier is USD 100 -->
				<multiplier>100</multiplier>
				<expirationDate id="ExpirationDate">
					<adjustableDate>
						<unadjustedDate>2006-06-17</unadjustedDate>
						<dateAdjustments>
							<businessDayConvention>NONE</businessDayConvention>
						</dateAdjustments>
					</adjustableDate>
				</expirationDate>
			</ext:option>
			
		</ext:optionContract>

	</trade>
	
	<party id="PartyA">
		<partyId>Party A</partyId>
	</party>
	<party id="PartyB">
		<partyId>Party B</partyId>
	</party>
	
</FpML>
